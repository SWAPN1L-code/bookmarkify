{
    "name": "bookmarkify-monorepo",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "dev:web": "npm run dev -w apps/web",
        "build:web": "npm run build -w apps/web",
        "dev:ext": "npm run dev -w apps/extension",
        "build:ext": "npm run build -w apps/extension",
        "dev:api": "npm run start:dev -w apps/api",
        "build:api": "npm run build -w apps/api",
        "lint": "npm run lint -w apps/web"
    }
}